<Page x:Class="BlocoDeNotas.Aplicativo.Editor"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BlocoDeNotas.Aplicativo"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Editor" Loaded="Page_Loaded">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="36"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="24"/>
        </Grid.RowDefinitions>
        <Frame Grid.Row="0" NavigationUIVisibility="Hidden" x:Name="FrameBarraMenu"/>
        <Frame Grid.Row="1" NavigationUIVisibility="Hidden" x:Name="FrameEditorDeDocumentos"/>
        <Frame Grid.Row="2" NavigationUIVisibility="Hidden" x:Name="FrameBarraDeStatus"/>
    </Grid>
</Page>
