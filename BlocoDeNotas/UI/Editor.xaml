<Page x:Class="BlocoDeNotas.UI.Editor"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BlocoDeNotas.UI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Editor" Loaded="Page_Loaded">

    <Grid Background="OliveDrab">
        <Grid.RowDefinitions>
            <RowDefinition Height="36"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="32"/>
        </Grid.RowDefinitions>
        <Frame Grid.Row="0" x:Name="BarraDeMenuFrame" NavigationUIVisibility="Hidden"/>
        <Frame Grid.Row="1" x:Name="EditorDeDocumentosFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Page>
